action (BuildPodcastAudioInfo) {
  type (Search)
  description (Makes a podcast audio info, aka a playlist, to play.)
  collect {
    input (podcastAudio) {
      type (audioPlayer.AudioItem)
      min (Required) max (Many)
      default-init {
        intent {
          goal: GetPodcasts
        }
      }
    }
  }
  output (audioPlayer.AudioInfo)
}
