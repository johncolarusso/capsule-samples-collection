action (FindPodcast) {
  type (Search)
  description (Finds podcast audio item using a searchTerm)
  collect {
    input (podcastAudio) {
      type (PodcastAudio)
      min (Optional) max (Many)
      default-init {
        intent {
          goal {
            GetPodcasts
          }
        }
      }
    }
    input (searchTerm) {
      type (SearchTerm)
      min (Optional) max (One)
    }
  }
  output (PodcastAudio)
}
