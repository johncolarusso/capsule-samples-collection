endpoints {
  action-endpoints {
    action-endpoint (PlayPodcast) {
      local-endpoint (PlayPodcast.js)
    }
    action-endpoint (BuildPodcastAudioInfo) {
      accepted-inputs (podcastAudio)
      local-endpoint (BuildPodcastAudioInfo.js)
    }
    action-endpoint (FindPodcast) {
      accepted-inputs (searchTerm)
      local-endpoint (FindPodcast.js)
    }
  }
}
